      </Button>
      <Button fx:id="button_clear2" onMouseClicked="#onMouseClicked_button2" layoutX="369.0" layoutY="154.0" mnemonicParsing="false" onMouseMoved="#onMouseMoved_button2" style="-fx-background-color: white;" text="×" textFill="#909090">
         <font>
            <Font size="7.0" />
         </font>
      </Button>
   </children>
