<AnchorPane fx:id="window_login" prefHeight="320.0" prefWidth="500.0" style="-fx-background-color: #FFFFFF;" styleClass="black" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jim.sprjfx.controller.MainController">
   <children>
